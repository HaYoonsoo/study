nyName,\n  ItemContainer,\n  Item,\n  MenuContainer,\n  Login,\n  CartWrapper,\n  Cart,\n} from \"./HeaderComponents\";\n\nconst Header = (props) => {\n  const [oldScroll, setOldScroll] = useState(window.scrollY);\n  const [isHidden, setIsHidden] = useState(false);\n\n  useEffect( () => {\n    window.addEventListener('scroll', (e) => {\n      setOldScroll( (prev) => {\n        const now = window.scrollY\n        if ( prev > now && now > 0) { //올라갔다. 튕겨서 내려오는 경우 제외\n          setIsHidden(false);\n        } else if ( prev < now && now > 0) { //내려갔다\n          setIsHidden(true);\n        }\n        return now;\n      });\n    })\n  }, []);\n\n  return (\n    <HeaderWrapper hide={isHidden}>\n      <HeaderContainer>\n        <CompanyName>WISELY</CompanyName>\n        <ItemContainer>\n          <Item to=\"#\">제품보기</Item>\n          <Item to=\"#\">브랜드 스토리</Item>\n          <Item to=\"#\">블로그</Item>\n          <Item to=\"#\">고객센터</Item>\n        </ItemContainer>\n        <MenuContainer>\n          <Login to=\"#\">로그인</Login>\n          <CartWrapper>\n            <Cart />\n          </CartWrapper>\n        </MenuContainer>\n      </HeaderContainer>\n    </HeaderWrapper>\n  );\n};\n\nexport default Header;\n","/Users/hys/Desktop/coding/wisely/src/components/MainBanner/index.js",["61","62"],[],"import React, { useEffect, useState } from \"react\";\nimport {\n  BannerWrapper,\n  BannerContainer,\n  BannerItem,\n  Banner,\n  ButtonContainer,\n  Button,\n  ButtonLeft,\n  ButtonRight,\n  ImageSequence,\n} from \"./MainBannerComponents\";\n\nimport ImageFile1 from \"../../assets/images/1.jpg\";\nimport ImageFile2 from \"../../assets/images/2.jpg\";\nimport ImageFile3 from \"../../assets/images/3.jpg\";\n\nconst MainBanner = () => {\n  const interval = 4000; //다음 이미지로 자동으로 넘어가는 시간\n  const [hover, setHover] = useState(false);\n  const [image, setImage] = useState(0);\n\n  useEffect(() => {\n    setInterval(() => showNextImage(), interval);\n  }, []);\n\n  function showNextImage() {\n    setImage((prev) => (prev + 1 > 2 ? 0 : prev + 1));\n  }\n\n  function showPreviousImage() {\n    setImage((prev) => (prev - 1 < 0 ? 2 : prev - 1));\n  }\n\n  return (\n    <BannerWrapper>\n      <BannerContainer>\n        <BannerItem image={image} bannerId=\"0\">\n          <Banner src={ImageFile1} />\n        </BannerItem>\n        <BannerItem image={image} bannerId=\"1\">\n          <Banner src={ImageFile2} />\n        </BannerItem>\n        <BannerItem image={image} bannerId=\"2\">\n          <Banner src={ImageFile3} />\n        </BannerItem>\n      </BannerContainer>\n      <ButtonContainer>\n        <Button onClick={showPreviousImage}>\n          <ButtonLeft />\n        </Button>\n        <Button onClick={showNextImage}>\n          <ButtonRight />\n        </Button>\n      </ButtonContainer>\n      <ImageSequence>{image + 1} / 3</ImageSequence>\n    </BannerWrapper>\n  );\n};\n\nexport default MainBanner;\n","/Users/hys/Desktop/coding/wisely/src/components/Main/index.js",[],[],"/Users/hys/Desktop/coding/wisely/src/components/Header/HeaderComponents.js",[],[],"/Users/hys/Desktop/coding/wisely/src/components/MainBanner/MainBannerComponents.js",[],[],"/Users/hys/Desktop/coding/wisely/src/components/Main/Promotion/index.js",[],[],"/Users/hys/Desktop/coding/wisely/src/components/Main/Promotion/PromotionComponents.js",[],[],"/Users/hys/Desktop/coding/wisely/src/components/Footer/FooterComponents.js",[],[],{"ruleId":"63","severity":1,"message":"64","line":15,"column":10,"nodeType":"65","messageId":"66","endLine":15,"endColumn":19},{"ruleId":"63","severity":1,"message":"67","line":20,"column":10,"nodeType":"65","messageId":"66","endLine":20,"endColumn":15},{"ruleId":"63","severity":1,"message":"68","line":20,"column":17,"nodeType":"65","messageId":"66","endLine":20,"endColumn":25},"no-unused-vars","'oldScroll' is assigned a value but never used.","Identifier","unusedVar","'hover' is assigned a value but never used.","'setHover' is assigned a value but never used."